    "build":{
        "env":{
            "NODE_ENV": "development"
        }
    },
    "builds": [
        {
            "src": "./index.js",
            "use": "@vercel/node"
        }

    ],
    "routes":[
        {
            "src":"/.*",
            "dest":"/index.js"
        }
    ]
}